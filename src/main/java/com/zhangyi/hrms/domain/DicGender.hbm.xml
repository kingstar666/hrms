<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhangyi.hrms.domain.DicGender" table="dic_gender" schema="hrms">
        <id name="genderId">
            <column name="gender_id" sql-type="int(11)"/>
        </id>
        <property name="genderName">
            <column name="gender_name" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="standard">
            <column name="standard" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="sequence">
            <column name="sequence" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="visible">
            <column name="visible" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>