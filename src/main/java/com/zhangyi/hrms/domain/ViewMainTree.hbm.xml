<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zhangyi.hrms.domain.ViewMainTree" table="view_main_tree" schema="hrms">
        <composite-id>
            <key-property name="id" type="java.lang.String" column="id"></key-property>
            <key-property name="type" type="java.lang.String" column="type"></key-property>
        </composite-id>

        <property name="pId" type="java.lang.String" column="pId"></property>
        <property name="name" type="java.lang.String" column="name"></property>
        <property name="sequence" type="java.lang.Integer" column="sequence"></property>
    </class>
</hibernate-mapping>